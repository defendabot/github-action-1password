{
  "name": "@defendabot/github-action-1password",
  "version": "1.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "npm run build && ncc build build/main.js",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run clean && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/defendabot/github-action-1password.git"
  },
  "keywords": [
    "github-actions",
    "github-secrets",
    "1password"
  ],
  "author": {
    "email": "vlad@defendabot.com",
    "url": "https://github.com/vladyslavvolkov",
    "name": "Vlad Volkov"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.6.0",
    "@actions/exec": "^1.0.4",
    "@actions/core": "^1.5.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/parser": "^3.8.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.6.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.2.2",
    "rimraf": "^3.0.2",
    "jest-circus": "^26.2.2",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  }
}
